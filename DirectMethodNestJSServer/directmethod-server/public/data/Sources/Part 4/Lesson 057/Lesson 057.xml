  <image/>
 </item>
 <item id="39" start="288.767" duration="1.6">
  <person>Teacher</person>
  <text>paint</text>
  <image/>
 </item>
 <item id="40" start="290.4" duration="2.033">
  <person>Teacher</person>
  <text>Can you paint?</text>
  <image/>
 </item>
 <item id="40" start="292.467" duration="2.167">
